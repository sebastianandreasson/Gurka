{
  "containerDefinitions": [
    {
      "name": "gurkapi",
      "image": "gurka/gurkapi:latest",
      "portMappings": [
        {
          "containerPort": 4000,
          "hostPort": 80
        }
      ],
      "secrets": [],
      "environment": [
        {
          "name": "TITLE",
          "value": "Gurkapi"
        },
        {
          "name": "AWS_REGION",
          "value": "eu-north-1"
        }
      ]
    }
  ],
  "executionRoleArn": "ecsTaskExecutionRole",
  "family": "gurkapi",
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "networkMode": "awsvpc",
  "cpu": "256",
  "memory": "1024"
}
